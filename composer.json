{
    "name": "winavin/permissions",
    "description": "A lightweight, Enum-driven Roles & Permissions package for Laravel, designed for multi-model and multi-team systems without requiring database-stored role or permission definitions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Navin Patil",
            "email": "winavin@gmail.com",
            "homepage": "https://github.com/winavin"
        }
    ],
    "homepage": "https://github.com/winavin/permissions",
    "keywords": [
        "php",
        "security",
        "laravel",
        "permissions",
        "roles",
        "authorization",
        "multiuser",
        "teams",
        "enums"
    ],
    "require": {
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Winavin\\Permissions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Winavin\\Permissions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Winavin\\Permissions\\PermissionsServiceProvider"
            ]
        }
    }
    
}